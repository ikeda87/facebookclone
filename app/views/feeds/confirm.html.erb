<br><br><h1>確認画面</h1><br>
<%= form_with(model: @feed, local: true) do |form| %>
　　<%= image_tag(@feed.image.url) if @feed.image && @feed.image.url %>

    <%= form.hidden_field :title %>
    <%= form.hidden_field :content %>
    <%= form.hidden_field :image_cache %>

　　<p>投稿内容:<%= @feed.content %></p>
　　<br><%= form.submit "投稿する" %>
    <% end %>
    <%= form_with(model:@feed, url:new_feed_path, local:true, method:"get") do |form| %>
    <br><%= form.submit "戻る", name: "back" %>
<% end %>
